<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.myctca.fragment.MoreMedDocCarePlanFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <WebView
            android:id="@+id/web_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"/>
        <include
            android:id="@+id/labs_bottom_button_layout"
            layout="@layout/bottom_button_layout" />
    </LinearLayout>

    <!-- Empty Table View -->
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/empty_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center"
        android:background="@color/white">

        <ImageView
            android:id="@+id/my_ctca_logo_image_view"
            android:layout_width="200dp"
            android:layout_height="220dp"
            android:contentDescription="@string/mail_empty_image_content_desc"
            app:srcCompat="@drawable/splash_9" />

        <TextView
            android:id="@+id/med_doc_care_plan_empty_message"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="0dp"
            android:gravity="center"
            android:text="@string/empty_list_message"
            android:textSize="15sp" />

    </LinearLayout>

</FrameLayout>
